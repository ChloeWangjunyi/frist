<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/listButton"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:layout_marginTop="50dp"
        android:background="@drawable/mine_id"
        android:gravity="center"
        android:text="列表显示"
        android:textColor="@color/blue" />

    <TextView
        android:id="@+id/fragmentButton"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        android:background="@drawable/mine_id"
        android:gravity="center"
        android:text="fragment"
        android:textColor="@color/blue" />

    <TextView
        android:id="@+id/sex"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        android:background="@drawable/mine_id"
        android:gravity="center"
        android:text="性别"
        android:textColor="@color/blue" />

    <Button
        android:id="@+id/editText"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        android:background="@drawable/mine_id"
        android:gravity="center"
        android:text="editText"
        android:textColor="@color/blue" />

    <TextView
        android:id="@+id/takePhoto"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        android:background="@drawable/mine_id"
        android:gravity="center"
        android:text="take a photo"
        android:textColor="@color/blue" />

    <TextView
        android:id="@+id/editTest"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        android:background="@drawable/mine_id"
        android:gravity="center"
        android:text="edit Test"
        android:textColor="@color/blue" />

    <TextView
        android:id="@+id/getOut"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        android:background="@drawable/mine_id"
        android:gravity="center"
        android:text="get out"
        android:textColor="@color/blue" />

    <TextView
        android:id="@+id/dialog"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        android:background="@drawable/mine_id"
        android:gravity="center"
        android:text="@string/dog_lee_s_dialog"
        android:textColor="@color/blue" />


    <TextView
        android:id="@+id/tel"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        android:background="@drawable/mine_id"
        android:gravity="center"
        android:paddingLeft="4dp"
        android:paddingRight="4dp"
        android:text="15764338952"
        android:textColor="@color/blue" />
</LinearLayout>